FQDN=matomo.example.com
ROOT_DIR=/var/data/matomo
TZ=Europe/Paris

MATOMO_VERSION=latest
TRAEFIK_VERSION=1.7-alpine
MARIADB_VERSION=10.2
REDIS_VERSION=4.0-alpine
SWARM_CRONJOB_VERSION=latest

TRAEFIK_LOG_LEVEL=INFO

MYSQL_ROOT_PASSWORD=
MYSQL_DATABASE=matomo
MYSQL_USER=matomo
MYSQL_PASSWORD=
MYSQL_MAX_CONNECTIONS=800
MYSQL_MAX_ALLOWED_PACKET=1024M
MYSQL_WAIT_TIMEOUT=28800
MYSQL_IDB_BUFFER_POOL_SIZE=8G
MYSQL_IDB_BUFFER_POOL_INSTANCES=8

MATOMO_REPLICAS=6
MATOMO_ARCHIVE_REQUESTS=10
MATOMO_CRON_ARCHIVE=0 0 * * * *
MATOMO_CRON_GEOIP=0 0 2 * * *
